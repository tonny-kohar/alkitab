<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE helpset PUBLIC "-//Sun Microsystems Inc.//DTD JavaHelp HelpSet Version 2.0//EN" "http://java.sun.com/products/javahelp/helpset_2_0.dtd">
<helpset version="2.0">
    <title>Alkitab User Guide</title>
    <maps>
        <homeID>kiyut.alkitab.modules.userguide</homeID>
        <mapref location="userguide-map.xml"/>
    </maps>
    <view mergetype="javax.help.AppendMerge">
        <name>TOC</name>
        <label>Table of Contents</label>
        <type>javax.help.TOCView</type>
        <data>userguide-toc.xml</data>
    </view>
    <view mergetype="javax.help.AppendMerge">
        <name>Index</name>
        <label>Index</label>
        <type>javax.help.IndexView</type>
        <data>userguide-idx.xml</data>
    </view>
    <!-- BUG: Apache Netbeans no longer include jhall.jar
        workaround, include jhall.jar directly
    -->
    <view>
        <name>Search</name>
        <label>Search</label>
        <type>javax.help.SearchView</type>
        <data>JavaHelpSearch</data>
    </view>
    <presentation default="true">
        <name>main window</name>
        <toolbar>
            <helpaction image="helpviewer_back">javax.help.BackAction</helpaction>
            <helpaction image="helpviewer_forward">javax.help.ForwardAction</helpaction>
            <helpaction image="helpviewer_print">javax.help.PrintAction</helpaction>
            <helpaction image="helpviewer_page-setup">javax.help.PrintSetupAction</helpaction>
        </toolbar>
    </presentation>
</helpset>
